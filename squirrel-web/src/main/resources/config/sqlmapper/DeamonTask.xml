<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dianping.cache.dao.TaskDao">

    <resultMap id="Task" type="com.dianping.cache.entity.Task">
        <result column="id" property="id"/>
        <result column="stat" property="stat"/>
        <result column="statMin" property="statMin"/>
        <result column="statMax" property="statMax"/>
        <result column="type" property="type"/>
        <result column="commitTime" property="commitTime"/>
        <result column="startTime" property="startTime"/>
        <result column="endTime" property="endTime"/>
        <result column="description" property="description"/>
    </resultMap>

    <insert id="insert" parameterType="Task">
        INSERT INTO task(stat, statMin, statMax, type, commitTime, startTime, endTime, description)
        VALUES(#{stat}, #{statMin}, #{statMax}, #{type}, #{commitTime}, #{startTime}, #{endTime}, #{description})
    </insert>

</mapper>